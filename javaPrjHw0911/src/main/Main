package main;

import java.util.ArrayList;

public class Main {

public static void main(String[] args) {
		System.out.println("복습~")
		
		//여러개의 객체를 관리할 변수
		ArrayList<Menu>menuList = new ArrayList<Menu>();
		
		//메뉴 객체 (데이터는 아무렇게나) 여러개 만들고, 위에서 만든 컬렉션에 담기
		menuList.add(new Menu ("아메리카노", 1500));
		menuList.add(new Menu ("카페라떼", 2000));
		menuList.add(new Menu ("카푸치노", 2500));
		
		//여러개의 객체 전부 다 출력 (모든 필드 값)
		for(int i = 0 ; i < menuList.size() ; i ++) {
			Menu x = menuList.get(i);
			System.out.println();
		}
		
	} //main
}//class
